			<script>
import java.util.regex.Pattern;
import java.util.regex.Matcher;

int maxAllowedComplexity = 5;
Pattern pattern = Pattern.compile(&quot;.*Cyclomatic Complexity is (\\d+).*&quot;);
String description = null;

if(hudson.model.Result.FAILURE.equals(manager.build.getResult())){
 if(manager.logContains(&quot;.*cp.*\\.java.*No such file or directory.*&quot;)){
  description = &quot;Fehlender Parameter!&quot;;
 }
 else if(manager.logContains(&quot;.*Compile failed.*&quot;)){
  description = &quot;Kompilierfehler - Benennung pr√ºfen!&quot;;
 }
}
else{
 int maxFound = 0;
 manager.build.logFile.eachLine { line -&gt;
  Matcher matcher = pattern.matcher(line)
  if(matcher.matches()) {
   maxFound = Math.max(maxFound, Integer.parseInt(matcher.group(1)));
  }
 }
 String addition = (maxFound &gt; 0) ? String.valueOf(maxFound) : &quot;&lt;=&quot; + maxAllowedComplexity;
 description = &quot;Zyklomatische Komplexit&#228;t: &quot; + addition;
}
manager.build.setDescription(description);
			</script>
			<sandbox>false</sandbox>
		</script>
